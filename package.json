{
  "name": "subreddit-filter",
  "private": true,
  "version": "1.0.65",
  "description": "Greasemonkey UserScript to filter subreddits from your reddit feed.",
  "keywords": [
    "userscript",
    "greasemonkey",
    "reddit"
  ],
  "homepage": "https://github.com/stevehartwell/subreddit-filter#readme",
  "bugs": {
    "url": "https://github.com/stevehartwell/subreddit-filter/issues"
  },
  "author": {
    "name": "Steve Hartwell",
    "email": "steve.hartwell@gmail.com",
    "url": "https://github.com/stevehartwell"
  },
  "license": "MIT",
  "source": "src/index.ts",
  "userscript": "dist/subreddit-filter.user.js",
  "browserslist": [
    "since 2018"
  ],
  "targets": {
    "userscript": {
      "context": "browser",
      "outputFormat": "global",
      "optimize": true
    }
  },
  "scripts": {
    "test": "echo 'Error: no tests yet' && exit 1",
    "develop": "npx parcel build --no-optimize --log-level info",
    "build": "npx parcel build --detailed-report"
  },
  "workspaces": [
    "src",
    "parcel-plugins/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stevehartwell/subreddit-filter.git"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.7.0",
    "@types/greasemonkey": "^4.0.3",
    "parcel": "^2.7.0"
  }
}
